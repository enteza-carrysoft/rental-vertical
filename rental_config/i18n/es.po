msgid "Rental Off-Days"
msgstr "Días libres de alquiler"

#. module: rental_config
#: model_terms:ir.ui.view,arch_db:rental_config.res_config_settings_view_form
msgid "Rental Options"
msgstr "Opciones de alquiler"

#. module: rental_config
#: model:ir.model.fields,field_description:rental_config.field_res_config_settings__module_rental_pricelist
msgid "Rental Prices"
msgstr "Precios de alquiler"

#. module: rental_config
#: model:ir.model.fields,field_description:rental_config.field_res_config_settings__module_rental_product_pack
msgid "Rental Product Packs"
msgstr "Packs de productos de alquiler"

#. module: rental_config
#: model:ir.model.fields,field_description:rental_config.field_res_config_settings__module_rental_product_set
msgid "Rental Product Sets"
msgstr "Kits de productos de alquiler"

#. module: rental_config
#: model:ir.model.fields,field_description:rental_config.field_res_config_settings__module_rental_repair
msgid "Repair Orders for Product Instances"
msgstr "Ordenes de reparación para instancias de producto"

#. module: rental_config
#: model:ir.model.fields,field_description:rental_config.field_res_config_settings__module_rental_timeline
msgid "Timeline"
msgstr "Línea temporal"

#. module: rental_config
#: model_terms:ir.ui.view,arch_db:rental_config.res_config_settings_view_form
msgid "Use contracts for periodic invoicing of monthly rentals"
msgstr "Usa contratos para la facturación periódica de alquileres mensuales"

#. module: rental_config
#: model_terms:ir.ui.view,arch_db:rental_config.res_config_settings_view_form
msgid "Use timeline for rentable products with related orders"
